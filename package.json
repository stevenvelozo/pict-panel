{
    "name": "pict-panel",
    "version": "1.0.6",
    "description": "Pict Control Panel",
    "main": "source/Pict-Panel.js",
    "scripts": {
        "start": "node source/Pict-Panel.js",
        "test": "npx quack test",
        "tests": "npx quack test -g",
        "coverage": "npx quack coverage",
        "build": "npx quack build"
    },
    "author": "steven <steven@velozo.com>",
    "license": "MIT",
    "devDependencies": {
        "pict": "^1.0.348",
        "pict-section-form": "^1.0.189",
        "quackage": "^1.0.58"
    },
    "dependencies": {
        "fable-serviceproviderbase": "^3.0.19",
        "pict-provider": "^1.0.12",
        "pict-template": "^1.0.15",
        "pict-view": "^1.0.67"
    },
    "mocha": {
        "diff": true,
        "extension": [
            "js"
        ],
        "package": "./package.json",
        "reporter": "spec",
        "slow": "75",
        "timeout": "5000",
        "ui": "tdd",
        "watch-files": [
            "source/**/*.js",
            "test/**/*.js"
        ],
        "watch-ignore": [
            "lib/vendor"
        ]
    }
}
