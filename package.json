{
    "name": "pict-panel",
    "version": "1.0.5",
    "description": "Pict Control Panel",
    "main": "source/Pict-Panel.js",
    "scripts": {
        "start": "node source/Pict-Panel.js",
        "test": "npx mocha -u tdd -R spec",
        "tests": "npx mocha -u tdd --exit -R spec --grep",
        "coverage": "npx quack coverage",
        "build": "npx quack build"
    },
    "author": "steven <steven@velozo.com>",
    "license": "MIT",
    "devDependencies": {
        "pict": "^1.0.348",
        "pict-section-form": "^1.0.189",
        "quackage": "^1.0.51"
    },
    "dependencies": {
        "fable-serviceproviderbase": "^3.0.18",
        "pict-provider": "^1.0.10",
        "pict-template": "^1.0.14",
        "pict-view": "^1.0.66"
    },
    "mocha": {
        "diff": true,
        "extension": [
            "js"
        ],
        "package": "./package.json",
        "reporter": "spec",
        "slow": "75",
        "timeout": "5000",
        "ui": "tdd",
        "watch-files": [
            "source/**/*.js",
            "test/**/*.js"
        ],
        "watch-ignore": [
            "lib/vendor"
        ]
    }
}
